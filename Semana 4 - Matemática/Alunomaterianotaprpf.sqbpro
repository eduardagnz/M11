mn index="1" value="61"/><column index="2" value="89"/><column index="3" value="205"/><column index="4" value="122"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1">CREATE TABLE aluno (
id INTEGER PRIMARY KEY,
name VARCHAR(255) NOT NULL,
idade INTEGER NOT NULL,
document_number CHAR(14) NOT NULL,

UNIQUE (document_number)
);

CREATE TABLE matéria (
id INTEGER PRIMARY KEY,
name VARCHAR(255) NOT NULL,
);

CREATE TABLE nota (
id INTEGER PRIMARY KEY,
aluno_id INTEGER NOT NULL REFERENCES aluno(id),
matéria_id INTEGER NOT NULL REFERENCES matéria(id),
nota DECIMAL(2, 2) NOT NULL

);

CREATE TABLE professor (
id INTEGER PRIMARY KEY,
name VARCHAR(255) NOT NULL,
document_number CHAR(14) NOT NULL,
matéria_id INTEGER NOT NULL REFERENCES matéria(id),

UNIQUE (document_number)
);</sql><sql name="SQL 2">insert into aluno (name, idade, document_number)
