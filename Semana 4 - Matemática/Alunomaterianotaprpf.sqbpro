FROM aluno a
CROSS JOIN matéria m
LEFT JOIN nota n ON a.id = n.aluno_id AND m.id = n.matéria_id
WHERE n.nota IS NULL;</sql><sql name="SQL 6">SELECT a.name AS &quot;Aluno&quot;, m.name AS &quot;Matéria&quot;, n.nota
