join professor p on p.matéria_id = m.id
where a.name = 'Duda'
</sql><sql name="SQL 5">SELECT a.name AS &quot;Aluno&quot;, m.name AS &quot;Matéria&quot;, n.nota
FROM nota n
JOIN aluno a ON n.aluno_id = a.id
JOIN matéria m ON n.matéria_id = m.id;

